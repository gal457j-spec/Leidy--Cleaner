{
  "name": "leidy-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Frontend Next.js para Leidy Cleaner",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next build && next export",
    "start": "next start",
    "lint": "eslint src __mocks__ *.config.js *.config.ts --ext .js,.jsx,.ts,.tsx",
    "clean:next": "pkill -f '[n]ext dev' || true && rm -rf .next && rm -f .next/dev/lock && echo CLEAN_DONE",
    "pretest:e2e": "npm run clean:next && fuser -k 3001/tcp || true && (cd ../backend && NODE_ENV=test PORT=3001 DB_TYPE=sqlite npm run dev:test &) && sleep 2",
    "test:e2e": "npx playwright test",
    "e2e:ci": "xvfb-run --auto-servernum npx playwright test --reporter=list",
    "cypress:open": "cypress open --project ./frontend",
    "cypress:run": "cypress run --project ./frontend",
    "test": "jest"
  },
  "dependencies": {
    "@react-google-maps/api": "^2.20.8",
    "@types/nodemailer": "^7.0.11",
    "@types/react-dom": "^19.2.3",
    "axios": "^1.13.5",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.34.3",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.564.0",
    "next": "^16.1.6",
    "nodemailer": "^8.0.1",
    "pg": "^8.18.0",
    "react": "^19.2.4",
    "react-colorful": "^5.6.1",
    "react-datepicker": "^9.1.0",
    "react-dom": "^19.2.4",
    "react-dropzone": "^15.0.0",
    "react-hot-toast": "^2.6.0",
    "socket.io-client": "^4.8.3",
    "tailwind-merge": "^3.5.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^10.0.1",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/forms": "^0.5.11",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.3.0",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.14",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "autoprefixer": "^10.4.24",
    "critters": "^0.0.23",
    "cypress": "^15.10.0",
    "eslint": "^10.0.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "playwright": "^1.58.2",
    "tailwindcss": "^3.4.1",
    "typescript": "5.9.3"
  }
}
