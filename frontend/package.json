{
  "name": "leidy-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Frontend Next.js para Leidy Cleaner",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next build && next export",
    "start": "next start",
    "lint": "eslint src __mocks__ *.config.js *.config.ts --ext .js,.jsx,.ts,.tsx",
    "clean:next": "pkill -f 'next dev' || true && rm -rf .next && rm -f .next/dev/lock",
    "pretest:e2e": "npm run clean:next",
    "test:e2e": "npx playwright test",
    "cypress:open": "cypress open --project ./frontend",
    "cypress:run": "cypress run --project ./frontend",
    "test": "jest"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
